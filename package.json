{
  "name": "rtk",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@reduxjs/toolkit": "^1.9.3",
    "date-fns": "^2.29.3",
    "miragejs": "^0.1.47",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.1",
    "react-scripts": "^5.0.1",
    "seedrandom": "^3.0.5",
    "txtgen": "^3.0.4"
  },
  "scripts": {
    "build:sass": "sass src/index.scss src/index.css",
    "watch:sass": "chokidar 'src/*.scss' 'src/*/*.tsx' --ignore 'src/*.css' -c 'yarn run build:sass'",
    "start": "npm-run-all build:sass --parallel start:react",
    "build": "npm-run-all build:sass --parallel build:react",
    "start:react": "react-scripts start",
    "build:react": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.1",
    "@types/react-router-dom": "^5.3.3",
    "chokidar-cli": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.58.3",
    "typescript": "^4.9.5"
  }
}
